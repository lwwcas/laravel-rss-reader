{
    "name": "lwwcas/laravel-rss-reader",
    "description": "The most complete RSS reader for laravel",
    "homepage": "https://github.com/lwwcas/laravel-rss-reader",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{
        "name": "Lucas Duarte Oliveira",
        "email": "lwwcas@gmail.com",
        "homepage": "http://www.lucasduarte.dev",
        "role": "Developer"
    }],
    "keywords": [
        "lwwcas",
        "Lucas Duarte",
        "laravel",
        "rss",
        "rss reader",
        "rss feed reader",
        "feed reader",
        "laravel reader",
        "laravel rss",
        "laravel rss feed",
        "laravel feed",
        "laravel feed reader",
        "laravel-rss-reader",
        "rss-reader"
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Lwwcas\\LaravelRssReader\\": "src",
            "Lwwcas\\LaravelRssReader\\Providers\\": "src/providers",
            "Lwwcas\\LaravelRssReader\\Models\\": "src/models",
            "Lwwcas\\LaravelRssReader\\Casts\\": "src/casts",
            "Lwwcas\\LaravelRssReader\\Database\\": "src/database",
            "Lwwcas\\LaravelRssReader\\Database\\Seeders\\": "src/database/seeders",
            "Lwwcas\\LaravelRssReader\\Database\\Seeders\\Countries\\": "src/database/seeders/countries"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lwwcas\\LaravelRssReader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lwwcas\\LaravelRssReader\\Providers\\LaravelRssReaderServiceProvider"
            ],
            "aliases": {
                "LaravelRssReader": "Lwwcas\\LaravelRssReader\\LaravelRssReaderFacade"
            }
        }
    }
}
